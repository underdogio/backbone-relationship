{
  "name": "backbone-relationship",
  "description": "Backbone plugin that coerces attributes to models and collections",
  "version": "1.0.0",
  "homepage": "https://github.com/underdogio/backbone-relationship",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/underdogio/backbone-relationship.git"
  },
  "bugs": {
    "url": "https://github.com/underdogio/backbone-relationship/issues"
  },
  "license": "MIT",
  "main": "lib/backbone-relationship",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "build-browserify": "browserify lib/browser.js > dist/backbone-serialize.js",
    "build-uglify": "uglifyjs dist/backbone-serialize.js > dist/backbone-serialize.min.js",
    "build": "npm run build-browserify && npm run build-uglify",
    "lint": "twolfson-style lint lib/ test/",
    "test": "mocha --reporter dot && npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "backbone": "~1.2.1",
    "browserify": "~10.2.4",
    "jscs": "~1.7.3",
    "jshint": "~2.5.10",
    "mocha": "~1.11.0",
    "twolfson-style": "~1.6.0",
    "uglify-js": "~2.4.23",
    "underscore": "~1.8.3"
  },
  "keywords": [
    "backbone",
    "plugin",
    "relationship",
    "relational",
    "inheritance"
